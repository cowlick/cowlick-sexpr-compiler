{
  "name": "@cowlick/sexpr-compiler",
  "version": "0.0.2",
  "generators" : [
    {
      "name" : "ocamllex" ,
      "command" : "ocamllex $in"
    },
    {
      "name" : "menhir" ,
      "command" : "menhir $in"
    }
  ],
  "sources": [
    {
      "dir": "src",
      "generators" : [
        {
          "name" : "ocamllex",
          "edge" : ["lexer.ml", ":", "lexer.mll"]
        },
        {
          "name" : "menhir",
          "edge" : ["parser.ml", "parser.mli", ":", "parser.mly"]
        }
      ]
    },
    {
      "dir": "__tests__",
      "type": "dev"
    }
  ],
  "package-specs": [
    "commonjs",
    "es6"
  ],
  "refmt": 3,
  "suffix": ".js",
  "bs-dependencies": [
    "minimist.re"
  ],
  "bs-dev-dependencies": [
    "@glennsl/bs-jest",
    "bs-glob"
  ]
}
